<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="expertPanel"  prefHeight="500.0" prefWidth="500.0"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ExpertController">
    <JFXButton fx:id="yes" layoutX="40.0" layoutY="33.0" mnemonicParsing="false" onAction="#calculate"
               text="Yes"/>
    <JFXButton fx:id="no" layoutX="102.0" layoutY="33.0" mnemonicParsing="false" onAction="#calculate"
               styleClass="button-raised" text="No"/>
    <Label fx:id="question" layoutX="91.0" layoutY="3.0" text="?"/>
    <JFXButton fx:id="new" layoutX="40.0" layoutY="66.0" mnemonicParsing="false" onAction="#clear" text="Заново"/>
    <JFXButton fx:id="input" layoutX="102.0" layoutY="66.0" mnemonicParsing="false" onAction="#nextPane"
               text="Режим обучения"/>
</AnchorPane>
