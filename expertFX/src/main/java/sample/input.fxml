<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="expertPanel" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.102"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InputController">
    <JFXListView fx:id="list" layoutY="113.0" prefHeight="300.0" prefWidth="500.0"/>
    <TextField fx:id="characteristicsTextField" layoutX="136.0" layoutY="14.0"/>
    <TextField fx:id="subjectsTextField" layoutX="136.0" layoutY="65.0"/>
    <Button fx:id="characteristicsAddButton" layoutX="391.0" layoutY="14.0" mnemonicParsing="false" text="Add" onAction="#addCharacteristic"/>
    <Button fx:id="subjectsAddButton" layoutX="391.0" layoutY="65.0" mnemonicParsing="false" text="Add" onAction="#addSubject"/>
    <Label layoutX="14.0" layoutY="20.0" text="Characteristics"/>
    <Label layoutX="14.0" layoutY="71.0" text="Subjects"/>
    <Button fx:id="input" layoutX="332.0" layoutY="420.0" mnemonicParsing="false" onAction="#nextPane" text="Ok"/>

</AnchorPane>
